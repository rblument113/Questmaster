<h1>List of Charcters</h1>

<table>
    <tr>
       <th>Name</th> 
       <th>Class</th> 
       <th>Level</th> 
    </tr>
    <% @characters.each do |character| %>
    <tr>
        <td><%= character.charname %></td>
        <td><%= character.charclass %></td>
        <td><%= character.level %></td>
        <td><%= link_to 'View', character_path(character)%></td>
    </tr>
    <% end %>
</table>